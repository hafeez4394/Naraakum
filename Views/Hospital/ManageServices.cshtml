
@{
    ViewBag.Title = "Manage Services";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="accordion">
    </div>
    <div class="formrow mt-4 text-center" style="margin-left:20px;">
        <button type="submit" id="SignUp" class="btn btn-primary" onclick="NK.ManageServices.saveData();">Save</button>
    </div>
    <div style="display:none" id="cardTemplate">
        <div class="card">
            <div class="card-header" id="headingOne">
                <h5 class="mb-0">
                    <input type="text" id="OrganizationId" value="@ViewBag.OrganizationId" />
                    <button class="btn btn-link mainHeader" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    </button>
                </h5>
            </div>

            <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                <div class="card-body">

                </div>
            </div>
        </div>
    </div>

@section Scripts{
    <script src=@Url.Content("~/js/Hospital/ManageServices.js")></script>
}